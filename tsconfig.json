{
  "compilerOptions": {
    "target": "ES2015",
    "module": "esnext",
    "strict": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators":true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "downlevelIteration":true,
    "declaration": false,
    "baseUrl": ".",
    "newLine": "lf",
    "paths": {
      "copy/*": ["packages/copy/src/*"]
    },
    "types": [ "@types/node" ],
    "jsx": "react-jsx",
  },
  "exclude": [ "node_modules" ]
}
